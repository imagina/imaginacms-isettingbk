{
  "name": "imagina/isetting-module",
  "type": "asgard-module",
  "description": "Setting module for AsgardCMS. Handles all the site settings.",
  "keywords": [
    "asgardcms",
    "settings"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dev Imagina",
      "email": "dev@imaginacolombia.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "dev@imaginacolombia.com",
    "issues": "https://github.com/imagina/imaginacms-platform/issues",
    "source": "https://github.com/imagina/imaginacms-isetting"
  },
  "require": {
    "php": ">=7.0.0",
    "composer/installers": "~1.0",
    "doctrine/dbal": "~2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "3.5.*"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Setting\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
